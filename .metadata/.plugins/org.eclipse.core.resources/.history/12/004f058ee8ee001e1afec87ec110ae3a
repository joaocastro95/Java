package cursoProgramacao;

import java.util.Scanner;

public class while2 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);

		int x = 4;
		int y = 0;
		int i = 0;

		while (i < x) {
			i = i + 1;
			y = y + i;
			System.out.println(i + "-" + y);
			
		}
		sc.close();

	}


}
